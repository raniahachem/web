<!DOCTYPE html>

<title>Client</title>

{% block body %}
    <h1>Client</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ client.id }}</td>
            </tr>
            <tr>
                <th>Cin_client</th>
                <td>{{ client.cinClient }}</td>
            </tr>
            <tr>
                <th>Nom_client</th>
                <td>{{ client.nomClient }}</td>
            </tr>
            <tr>
                <th>Prenom_client</th>
                <td>{{ client.prenomClient }}</td>
            </tr>
            <tr>
                <th>Ville_client</th>
                <td>{{ client.villeClient }}</td>
            </tr>
            <tr>
                <th>Telephone_client</th>
                <td>{{ client.telephoneClient }}</td>
            </tr>
            <tr>
                <th>Email_client</th>
                <td>{{ client.emailClient }}</td>
            </tr>
            <tr>
                <th>Mdp_client</th>
                <td>{{ client.mdpClient }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_client_index') }}">back to list</a>

    <a href="{{ path('app_client_edit', {'id': client.id}) }}">edit</a>

    {{ include('client/_delete_form.html.twig') }}
